            "Value": "SetVar(col_code, col-md-9 mt-sm text-right pre)\nSetVar(col_btn, col-md-3 mt-sm text-left)\nSetVar(row, form-group clearfix)\n\nSetTitle($demo_title$)\nDiv(content-wrapper){\n    Div(row){\n        Div(col-lg-8 col-lg-offset-2){\n            If(#method#==\"sendPage\"){\n                Div(h4){LangRes(input)}\n                Div(Class: panel panel-primary){\n                    Form(panel-body input-group){\n                        Input(Name: SomeText, Type:text, Placeholder:Enter text)\n                        Div(input-group-btn){\n                            Button(Body:Send to Page, Page:demo_page, PageParams: \"pageValue=Val(SomeText),method=sendPage\", Class:btn btn-primary)\n                        }\n                    }\n                }\n                \n                \n                Div(h4){LangRes(result)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        pageValue : Span(#pageValue#).Style(color:darkgreen;font-weight: bold;)\n                    }\n                }\n                \n                Div(h4){LangRes(code)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        P(\"Fоrm(){\")\n                        P(\"Inрut(Name: Span(SomeText).Style(color:darkred;font-weight:bold;), Type:text, Placeholder:Enter text)\")\n                        \n                        P(\"Вutton(Body:Send to Page, Page:demo_page, PageParams: `pageValue=Val(SomeText),method=sendPage`)\")\n                        P(\"}\")\n                    }\n                }\n                \n                Div(h4){LangRes(description)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        LangRes(demo_description_sendPage)\n                    }\n                }\n            }\n            \n            If(#method#==\"sendContract\"){\n                Div(h4){LangRes(input)}\n                Div(Class: panel panel-primary){\n                    Form(panel-body input-group){\n                        Input(Name: SomeText, Type:text, Placeholder:Enter text)\n                        Div(input-group-btn){\n                            Button(Body:Send to Contract, Contract: DemoContract, Page: demo_page, PageParams: \"method=sendContract\", Class:btn btn-primary)\n                        }\n                    }\n                }\n                \n                \n                Div(h4){LangRes(result)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        DBFind(Name:demo_page_table, Source: my_table)\n                        Div(table-responsive){\n                            Table(Source: my_table, Columns:\"id=member_id,name=member_name,text=some_text\")\n                        }\n                    }\n                }\n                \n                Div(h4){LangRes(code)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        P(\"Fоrm(){\")\n                        P(\"Inрut(Name: Span(SomeText).Style(color:darkred;font-weight:bold;), Type:text, Placeholder:Enter text)\")\n                        P(\"Buttоn(Body:Send to Contract, Cоntract: DemoContract, Page: demo_page)\")\n                        P(\"}\")\n                        \n                        P(\"DВFind(Name:demo_page_table, Source: Span(my_table).Style(color:darkblue;font-weight:bold;))\")\n                        P(\"Тable(Source: Span(my_table).Style(color:darkblue;font-weight:bold;), Columns:`id=member_id,name=member_name,text=some_text`)\")\n                    }\n                }\n                \n                Div(h4){LangRes(description)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        LangRes(demo_description_sendContract)\n                    }\n                }\n            }\n            \n            \n            \n            If(Or(#method#==\"sendMessage\",#notific_id#>0)){\n                Div(h4){LangRes(input)}\n                Div(Class: panel panel-primary){\n                    Form(panel-body){\n                        Div(col-sm-6){\n                            Input(Name: SomeText, Type:text, Placeholder:Enter text)\n                        }\n                        Div(col-sm-6){\n                            Div(input-group){\n                                DBFind(Name: member, Source: src_member)\n                                Select(Name: MemberId, Source: src_member, NameColumn: member_name, ValueColumn: member_id)\n                                Div(input-group-btn){\n                                    Button(Body: Send Notification, Contract: Notifications_Single_Send, Class:btn btn-primary, Params: \"member_id=Val(MemberId),text_header=You have a message,page_name=demo_page,val_str=Val(SomeText)\")\n                                }\n                            }\n                        }\n                    }\n                }\n                \n                Div(h4){LangRes(result)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        If(#notific_id#>0){\n                            Div(row){\n                                Div(col-sm-6 col-sm-offset-3 alert alert-info){\n                                    Div(){text_header: #notific_header#}\n                                    Div(){text_body: #notific_body#}\n                                    Div(){val_int: #notific_val_int#}\n                                    Div(){val_str: #notific_val_str#}\n                                    Div(pull-right text-right){\n                                        Button(Page: demo_page, Class: btn btn-default, Contract:Notifications_Single_Close, Params:\"notific_id=#notific_id#\", Body: OK)\n                                    }\n                                }\n                                \n                            }.Style(display: flex;align-items: flex-end;)\n                        }\n                    }\n                }\n                \n                Div(h4){LangRes(code)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        If(#notific_id#>0){\n                            P(\"Вutton(Page: demo_page, Body: OK, Contract:Notifications_Single_Close, Params:`notific_id=#notific_id#`)\")\n                        }\n                        P(\"Fоrm(){\")\n                        P(\"Inрut(Name: Span(SomeText).Style(color:darkred;font-weight:bold;), Type:text, Placeholder:Enter text)\")\n                        P(\"DВFind(Name: member, Source: Span(src_member).Style(color:darkgoldenrod;font-weight:bold;))\")\n                        P(\"Sеlect(Name: MemberId, Source: Span(src_member).Style(color:darkgoldenrod;font-weight:bold;), NameColumn: member_name, ValueColumn: member_id)\")\n                        P(\"Вutton(Body: Send Notification, Contract: Notifications_Single_Send, Params: `member_id=Val(MemberId),text_header=You have a message,val_str=Val(Span(SomeText).Style(color:darkred;font-weight:bold;)),page_name=demo_page`)\")\n                        P(\"}\")\n                    }\n                }\n                \n                Div(h4){LangRes(description)}\n                Div(Class: panel panel-primary){\n                    Div(panel-body){\n                        LangRes(demo_description_sendMessage)\n                    }\n                }\n            }\n            \n            If(#method#==\"sendPage\"){\n            }.ElseIf(#method#==\"sendContract\"){\n            }.ElseIf(Or(#method#==\"sendMessage\",#notific_id#>0)){\n            }.Else{\n                Div(h2 jumbotron){LangRes(demo_welcome)}\n            }\n        }\n    }\n}"
        }
