    "contracts": [
        {
            "Name": "DemoContract",
            "Value": "contract DemoContract {\n    data {\n        SomeText string\n    }\n    \n    conditions {\n        \n    }\n    \n    action {\n        $row_member = DBFind(\"member\").Where(\"member_id=?\", $key_id)\n        if(Len($row_member)){\n            $member = $row_member[0]\n            DBInsert(\"demo_page_table\", \"member_name,member_id,some_text\", $member[\"member_name\"], $member[\"member_id\"], $SomeText)\n        }else{\n            info \"Member not found\"\n        }\n    }\n}",
            "Conditions": "ContractConditions(\"MainCondition\")"
        }
    ],
