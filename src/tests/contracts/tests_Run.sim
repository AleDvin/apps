contract tests_Run {
    data{
        Date string "date"
    }
    conditions {
        var tests array
        tests[0] = "tests_JsonOperationDB"
        $tests = tests
    }
    action {
        var i int
        while i < Len($tests) {
            var success bool
            var params map
            success = CallContract($tests[i], params)
            if success {
                DBInsert("tests", "name,date,result", $tests[i], $Date, "ok")
            }else{
                DBInsert("tests", "name,date,result", $tests[i], $Date, "fail")
            }
            i=i+1
        }
        
    }
}