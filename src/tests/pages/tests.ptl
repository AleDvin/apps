Div(content-wrapper){
    Div(row){
        Div(col-sm-6 col-sm-offset-3){
            Form(panel panel-default){
                Div(panel-heading){
                    Input(Name: Date, Type: hidden, Value: Now("YYYY-MM-DD HH:MI:SS"))
                    Button(Contract: tests_Run, Class: btn btn-primary, Body: run tests, Page: tests, PageParams: "date=Val(Date)")
                }
                If(GetVar(date)){
                    Div(panel-body){
                        DBFind(Name: tests, Source: src).Where("date='#date#'").Order("id desc")
                        Div(form-group){
                            ForList(src){
                                If(#result#=="fail"){
                                    SetVar(hl, text-danger)
                                }.Else{
                                    SetVar(hl, text-success)
                                }
                                Div(row #hl#){
                                    Div(col-sm-6){#name#}
                                    Div(col-sm-5){DateTime(#date#)}
                                    Div(col-sm-1){#result#}
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}