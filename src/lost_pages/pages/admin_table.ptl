If(GetVar(name)){
    SetTitle(#name#)
    Div(content-wrapper){
        If(#sort#==id){
            AddToolButton(Icon: fa fa-sort, Page:admin_table, PageParams:"sort=id desc,name=#name#")
        }.Else{
            SetVar(sort, id desc)
            AddToolButton(Icon: fa fa-sort, Page:admin_table, PageParams:"sort=id,name=#name#")
        }
        DBFind(#name#, src).Limit(250).Order(#sort#)
        Div(table-responsive){
            Table(src)
        }
    }
}.Else{
    name not found
}