Div(breadcrumb){
	ForList(breadcrumbs){
		If(#name#!=""){
			If(#page#!=""){
				If(#params#!=""){
					LinkPage(Page: #page#, PageParams: #params#){#name#}
					Span(/).Style(margin-right: 10px; margin-left: 10px;)
				}.Else{
					LinkPage(Page: #page#){#name#}
					Span(/).Style(margin-right: 10px; margin-left: 10px;)
				}
			}.Else{
				Span(#name#, text-muted)
			}
		}
	}
}