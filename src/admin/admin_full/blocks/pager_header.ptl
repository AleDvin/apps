Div(){
    If(GetVar(#current_page#) == ""){
		SetVar(current_page, 1)
	}
    Div(){
        SetVar(pager_offset, Calculate(Exp: (#current_page# - 1) * 10, Type: int))
        SetVar(pager_limit, 10)
		SetVar(pager_lower_or_equal, Calculate(Exp: ((#current_page# - 1) * 10) + 10, Type: int))
        SetVar(current_page, #current_page#)
    }
}