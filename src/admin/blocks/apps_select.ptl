If(And(GetVar(this_page),#this_app_id#>0)){
    DBFind(applications,apps)
    Form(row form-group){
        Div(col-sm-4){
            Select(Name:AppId, Source:apps, NameColumn: name, ValueColumn: id, Value: #this_app_id#)
        }
        Div(col-sm-2){
            Button(Body:go, Page:#this_page#, Class:btn btn-default, PageParams: "this_app_id=Val(AppId)")
        }
    }
}