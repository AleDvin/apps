If(And(GetVar(this_page),#this_app_id#>0)){
    DBFind(applications,apps)
    Form(row form-group){
        Div(col-sm-4){
            Div(input-group){
                Select(Name:AppId, Source:apps, NameColumn: name, ValueColumn: id, Value: #this_app_id#)
                Div(input-group-btn){
                    Button(Page:#this_page#, Class:btn btn-default fa fa-arrow-right, PageParams: "this_app_id=Val(AppId)")
                }
            }
        }
    }
}