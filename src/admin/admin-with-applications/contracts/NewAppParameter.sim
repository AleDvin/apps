contract NewAppParameter {
		data {
			Name string
			Value string
			Conditions string
			App_id int
		}
		conditions {
			ValidateCondition($Conditions, $ecosystem_id)

			var row map
			row = DBRow("appparam").Columns("id").Where("name = ?", $Name)

			if row {
				warning Sprintf( "Parameter %s already exists", $Name)
			}
		}
		action {
			DBInsert("appparam", "app_id,name,value,conditions",$App_id,$Name, $Value, $Conditions )
		}
	}