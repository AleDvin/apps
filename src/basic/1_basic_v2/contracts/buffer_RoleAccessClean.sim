contract buffer_RoleAccessClean {
    data {}

    conditions {
        $bufferId = Int(DBFind("buffer_data").Where("key='roles_access' and member_id=?", $key_id).One("id"))
    }

    action {
        if $bufferId > 0 {
            var values map
            DBUpdate("buffer_data", $bufferId, "value", values)
        }
    }
}