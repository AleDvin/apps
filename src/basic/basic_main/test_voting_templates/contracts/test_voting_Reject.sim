contract test_voting_Reject{
    data{
        VotingTemplateId string "optional"
        KeyId string "optional"
        VotingId string "optional"
    }
    
    conditions{}
    
    action{
        var voting map
        voting = DBFind("voting_instances").Where("id=?", Int($VotingId)).Row()
        
        var values string
        values = Sprintf("test_voting_Reject\ntemplate:%v\nvoting:%v\n%v]", $VotingTemplateId, $VotingId, $KeyId)
        DBInsert("test", "app,case,value", "voting", voting["description"], values)
    }
}