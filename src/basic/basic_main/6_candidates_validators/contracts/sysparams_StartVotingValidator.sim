contract sysparams_StartVotingValidator{
    data{
        AnyId int
    }
    
    conditions{
        ContractConditions("MainCondition")
        // TODO проверить и отладить 
        $candidate = DBFind("sysparams_candidate").Where("id=? and deleted=0", $AnyId).Row()
        if !$candidate{
            warning "Candidate not found"
        }
        $KeyId = Int($candidate["candidate_key_id"])
        
        sysparams_DelegateRefresh()
    }
    action{}
}