contract supplychains_start
{
    data 
    {
        cargo_id int
    }

    conditions 
    {

    }

    action 
    {
		var supplies array
		supplies = DBFind("supplychains_supply").Columns("id").Where("cargo_id=?", $cargo_id)

        var supply map
        supply = supplies[0]

        var supply_id int
        supply_id = supply["id"]

        DBUpdateExt("supplychains_supply", "id", supply_id, "status", 3)
        
        supplychains_sendNotification("supply_id", supply_id)
    }
}