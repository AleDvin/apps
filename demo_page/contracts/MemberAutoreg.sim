contract MemberAutoreg {
    conditions {
    }
    
    action {
        var i int
        while true{
            $name = Sprintf("member_%v", i)
            $member = DBFind("member").Where("member_name=?", $name).Row()
            if !$member{
                break
            }
            i = i+1
        }        
        Profile_Edit("Username,Avatar", $name, "")
    }
}