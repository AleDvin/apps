contract MemberAutoreg {
    conditions {
        $id = 0
        $member = DBFind("member").WhereId($key_id).Row()
        if $member{
            $id = Int($member["id"])
        }
    }
    
    action {
        if $id == 0{
            DBInsert("member", "id,member_name", $key_id, "member")
        }
    }
}