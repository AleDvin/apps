contract MemberAdd {
    data {
        member_id int
    }
    
    action {
        $member_table = "member"
        $memberSelect = DBFind($member_table).Where("member_id = $", $member_id)
        if(Len($memberSelect) == 0){
            DBInsert($member_table, `member_id`, $member_id)
        }
    }
}